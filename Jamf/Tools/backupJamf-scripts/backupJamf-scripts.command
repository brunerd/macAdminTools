APIURL=""
APIUSER=""

######################
# COMPUTED VARIABLES #
######################

myFolderPath=$(dirname "${0}")
dateStampNow=$(date +"%Y%m%d-%H%M")

#############
# FUNCTIONS #
#############

function jpt()( #v1.0.9 jpt([options] [query] [fileArg])
{ set +x; } &> /dev/null
